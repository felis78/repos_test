(undef saveFunc (funcname fn?nil)
  (let fsrc (eval (makelist (+s "(func " funcname ")"))))
  (if (== fsrc nil) (return "This function does not exist..."))
  (if (== (car fsrc) nil) (let fsrc (replaceNth 1 fsrc (+s _openParent _closeParent))))
  (cond ((not fn) (let fn (+s funcname '.ll)))
        (t (let fn (+s fn '.ll)))
  )
  (let fh (file fn 'w)
  (let fh (file fn 'w))
  (fprintline fh fsrc)
  (fclose fh)
  (print funcname " saved in " ((fh.getContent).getName))
)
